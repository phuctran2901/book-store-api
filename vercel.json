{
  "version": 2,
  "name": "bookstore-api",
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "headers": [
    {
      "source": "/(.*)",
      "dest": "./index.js",
      "headers": [
        {
          "key": "access-control-allow-origin",
          "value": "*"
        }
      ]
    }
  ]
}
